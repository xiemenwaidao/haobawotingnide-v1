        "astro": "^2.2.3",
        "microcms-js-sdk": "^2.3.3",
        "remark-collapse": "^0.1.2",
        "remark-toc": "^8.0.1",
        "satori": "^0.4.8",
        "sharp": "^0.32.0",
        "swr": "^2.1.3",
        "tailwindcss": "^3.3.1"
    },
    "devDependencies": {
        "@astrojs/partytown": "^1.2.0",
        "@astrojs/react": "^2.1.1",
        "@astrojs/sitemap": "^1.2.2",
        "@astrojs/tailwind": "^3.1.1",
        "@divriots/jampack": "^0.9.4",
